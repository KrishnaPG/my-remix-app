{
  "name": "my-remix-app",
  "private": true,
  "description": "A Lightweight, Feature-Rich, and Production-Ready Remix Stack for your next SaaS application.",
  "repository": "dev-xo/remix-saas",
  "author": "Daniel Kanem <devxo@mail.com> (https://twitter.com/DanielKanem)",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "remix vite:build",
    "db:init": "npx prisma migrate dev --name init --skip-seed",
    "db:seed": "npx prisma db seed",
    "dev": "node ./server.mjs",
    "format": "prettier --write .",
    "lint": "eslint  --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production PORT=12000 node ./server.mjs",
    "start:stripeListener": "node_modules/.bin/stripe.exe  listen --forward-to localhost:12000/api/webhook",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc",
    "validate": "npm run lint && npm run typecheck"
  },
